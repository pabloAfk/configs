# SUPER = tecla Windows

key "Shift+Print" scrot
key "Print" scrot -s
key "Super+Q" Kill
key "Super+A" st
key "Super+D" 9menu -label Apps -bg black -fg white -font fixed "st:st" "fm:xfe" "pm:palemoon" "sober:flatpak run org.vinegarhq.Sober" "ds:dbus-launch flatpak run com.discordapp.Discord" "-" "audio:pavucontrol" "bt:blueman-manager" "brilho:st -e /home/dystopia/scripts/bright.sh" "wlan:st -e nmtui"
key "Super+N" rox
key "XF86AudioRaiseVolume" exec amixer set Master 5%+
key "XF86AudioLowerVolume" exec amixer set Master 5%-
key "XF86AudioMute" exec amixer set Master toggle
